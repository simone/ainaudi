  "scripts": {
    "start": "node backend/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix backend && npm run build",
    "dev": "concurrently \"npm run backend\" \"npm run pdf\" \"npm run frontend\"",
    "backend": "PORT=3001 nodemon backend/server.js",
    "pdf": "cd pdf && source m5s/bin/activate && python main.py",
    "frontend": "react-scripts start",
    "deploy": "npm run build && npm run install-backend && gcloud app deploy --no-cache --promote --quiet",
    "install-backend": "cd backend && npm install && cd .."
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
